#include <stdio.h>
#include<iostream>
using namespace std;
int main()
{	int x;
	int A[10]= {2,4,6,8,10,12};

	for(int i=0; i<10; i++) {
		printf("array elements are: %d \n",A[i]);
	}
	//by for each loop
	cout<<"elements of array by for each loop : ";
	for(int x:A) {
		cout<<x<<endl;
	}

	return 0;
}
