#include <iostream>
#include<stdio.h>
using namespace std;

int add(int a,int b)
{
	int c;
	c=a+b;

	return c;

}

int main ()
{
	int num1=10,num2=15,sum;

    sum = add(num1,num2);//value of num1 and num2 will be copied in a and b respectively 

	printf("Sum is : %d",sum); //change in formal parameters will not be reflected in actual parameters

	return 0;
}
